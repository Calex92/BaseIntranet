{% extends "@StatatorApp/layout.html.twig" %}

{% block content %}
    <div class="main-content">
        <div class="row">
            {{ parent() }}
            <div class="col-md-12">
                <h1>Profils connectés à l'application "{{ applicationName }}" sur les {{ lastMonth }} derniers mois</h1>
                <div class="col-md-offset-2 col-md-8" id="applicationChart"></div>
            </div>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
        {{ gc_draw(applicationChart, 'applicationChart') }}
    </script>
{% endblock %}
