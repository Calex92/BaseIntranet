<div>
    <h4 class="margin-left-15">Contact</h4>
    <div class="form-group">
        {{ form_label(form.contact.phone, null, {'label_attr' : {"class" : "col-sm-4 control-label"}} ) }}
        <div class="col-sm-8">
            {{ form_widget(form.contact.phone, {"attr": {'class' : "form-control"}}) }}
        </div>
        {{ include ('@FrontApp/Form/errorForm.html.twig', {'formValue' : form.contact.phone}) }}
    </div>

    <div class="form-group">
        {{ form_label(form.contact.mobilePhone, null, {'label_attr' : {"class" : "col-sm-4 control-label"}} ) }}
        <div class="col-sm-8">
            {{ form_widget(form.contact.mobilePhone, {"attr": {'class' : "form-control"}}) }}
        </div>
        {{ include ('@FrontApp/Form/errorForm.html.twig', {'formValue' : form.contact.mobilePhone}) }}
    </div>

    <div class="form-group">
        {{ form_label(form.contact.fax, null, {'label_attr' : {"class" : "col-sm-4 control-label"}} ) }}
        <div class="col-sm-8">
            {{ form_widget(form.contact.fax, {"attr": {'class' : "form-control"}}) }}
        </div>
        {{ include ('@FrontApp/Form/errorForm.html.twig', {'formValue' : form.contact.fax}) }}
    </div>
</div>